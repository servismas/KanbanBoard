<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BoardService" />
                <binding name="Card" />
                <binding name="Column" />
                <binding name="Profile" />
                <binding name="Team" />
                <binding name="User" />
                <binding name="AttachService" />
                <binding name="LogON" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8734/Design_Time_Addresses/service/Board"
                binding="basicHttpBinding" bindingConfiguration="BoardService"
                contract="BoardServiceReference.ICreateEditeBoardContract"
                name="BoardService" />
            <endpoint address="http://localhost:8735/Design_Time_Addresses/service/CardService"
                binding="basicHttpBinding" bindingConfiguration="Card" contract="CardServiceReference.ICreateEditeCardContract"
                name="Card" />
            <endpoint address="http://localhost:8736/Design_Time_Addresses/service/ColumnService"
                binding="basicHttpBinding" bindingConfiguration="Column" contract="ColumnServiceReference.ICreateEditColumnsContract"
                name="Column" />
            <endpoint address="http://localhost:8738/Design_Time_Addresses/service/ProfileService"
                binding="basicHttpBinding" bindingConfiguration="Profile"
                contract="ProFileServiceReference.ICreateEditeProfileContract"
                name="Profile" />
            <endpoint address="http://localhost:8739/Design_Time_Addresses/service/TeamService"
                binding="basicHttpBinding" bindingConfiguration="Team" contract="TeamServiceReference.ICreateEditeTeamContract"
                name="Team" />
            <endpoint address="http://localhost:8740/Design_Time_Addresses/service/UserService"
                binding="basicHttpBinding" bindingConfiguration="User" contract="UserServiceReference.ICreateEditeUserContract"
                name="User" />
            <endpoint address="http://localhost:8741/Design_Time_Addresses/service/Attach"
                binding="basicHttpBinding" bindingConfiguration="AttachService"
                contract="AttachmentServiceReference.ICreateEditAttachmentContract"
                name="AttachService" />
            <endpoint address="http://localhost:8737/Design_Time_Addresses/service/LogOnService"
                binding="basicHttpBinding" bindingConfiguration="LogON" contract="LogOnServiceReference.ILogOnUserContract"
                name="LogON" />
        </client>
    </system.serviceModel>
</configuration>